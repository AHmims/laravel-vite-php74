{
	"name": "innocenzi/laravel-vite",
	"description": "Vite integration for Laravel",
	"keywords": [
		"innocenzi",
		"laravel-vite"
	],
	"homepage": "https://github.com/innocenzi/laravel-vite",
	"license": "MIT",
	"authors": [
		{
			"name": "Enzo Innocenzi",
			"email": "enzo@innocenzi.dev",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.4",
		"guzzlehttp/guzzle": "^7.4.2",
		"illuminate/contracts": "8.83.5",
		"illuminate/support": "8.83.5",
		"spatie/laravel-package-tools": "^1.11.3"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^v3.8.0",
		"orchestra/testbench": "^v6.24.1",
		"pestphp/pest": "^v1.21.2",
		"phpunit/phpunit": "^9.5.19",
		"symfony/process": "^v5.4.5"
	},
	"autoload": {
		"files": [
			"src/helpers.php"
		],
		"psr-4": {
			"Innocenzi\\Vite\\": "src",
			"Innocenzi\\Vite\\Database\\Factories\\": "database/factories"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Innocenzi\\Vite\\Tests\\": "tests"
		}
	},
	"scripts": {
		"test": "pest"
	},
	"config": {
		"sort-packages": true,
		"allow-plugins": {
			"pestphp/pest-plugin": true
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Innocenzi\\Vite\\ViteServiceProvider"
			],
			"aliases": {
				"Vite": "Innocenzi\\Vite\\ViteFacade"
			}
		}
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
